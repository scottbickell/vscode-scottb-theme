	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#abb0b6"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"constant.other.symbol"
			],
			"settings": {
				"foreground": "#86b300"
			}
		},
		{
			"name": "Regular Expressions and Escape Characters",
			"scope": [
				"string.regexp",
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#4cbf99"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#ff9940"
			}
		},
		{
			"name": "Built-in constants",
			"scope": [
				"constant.language"
			],
			"settings": {
				"foreground": "#ff9940"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#6c7680"
			}
		},
		{
			"name": "Member Variable",
			"scope": [
				"variable.member"
			],
			"settings": {
				"foreground": "#f07171"
			}
		},
		{
			"name": "Language variable",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#338ccc"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage"
			],
			"settings": {
				"foreground": "#ff6600"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword"
			],
			"settings": {
				"foreground": "#ff6600"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#ff9900"
			}
		},
		{
			"name": "Separators like ; or ,",
			"scope": [
				"punctuation.separator",
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#6c7680b3"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.section"
			],
			"settings": {
				"foreground": "#6c7680"
			}
		},
		{
			"name": "Accessor",
			"scope": [
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#ff9900"
			}
		},
		{
			"name": "Types fixes",
			"scope": [
				"source.java storage.type",
				"source.haskell storage.type",
				"source.c storage.type"
			],
			"settings": {
				"foreground": "#338ccc"
			}
		},
		{
			"name": "Inherited class type",
			"scope": [
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#338ccc"
			}
		},
		{
			"name": "Lambda arrow",
			"scope": [
				"storage.type.function"
			],
			"settings": {
				"foreground": "#ff6600"
			}
		},
		{
			"name": "Java primitive variable types",
			"scope": [
				"source.java storage.type.primitive"
			],
			"settings": {
				"foreground": "#338ccc"
			}
		},
		{
			"name": "Function name",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "#f2ae49"
			}
		},
		{
			"name": "Function arguments",
			"scope": [
				"variable.parameter",
				"meta.parameter"
			],
			"settings": {
				"foreground": "#993399"
			}
		},
		{
			"name": "Function call",
			"scope": [
				"variable.function",
				"variable.annotation",
				"meta.function-call.generic",
				"support.function.go"
			],
			"settings": {
				"foreground": "#f2ae49"
			}
		},
		{
			"name": "Library function",
			"scope": [
				"support.function",
				"support.macro"
			],
			"settings": {
				"foreground": "#f07171"
			}
		},
		{
			"name": "Imports and packages",
			"scope": [
				"entity.name.import",
				"entity.name.package"
			],
			"settings": {
				"foreground": "#86b300"
